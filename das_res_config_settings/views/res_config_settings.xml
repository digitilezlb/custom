<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="das_res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.fire_base_api</field>
            <field name="priority" eval="200"/>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='integration']" position="after">
                    <div name="fire_base_api_key">
                        <h2>Fire Base Api Key</h2>
                        <div class="row mt16 o_settings_container">
                            <div class="col-xs-12 col-md-6 o_setting_box">
                                <div class="o_setting_right_pane">
                                    <!-- <label string="Configure your Google Maps View"/> -->

                                    <div class="content-group">
                                        <div class="mt16">
                                            <label for="fire_base_api_key" string="Fire Base Api Key"/>
                                            <field name="fire_base_api_key"/>
                                        </div>



                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
